<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="calculator" class="com.airwallex.rpncalculator.impl.CalculatorImpl">
        <property name="processor" ref="inputProcessor" />
        <property name="operandStackManager" ref="operandStackManager" />
    </bean>

    <bean id="inputProcessor" class="com.airwallex.rpncalculator.impl.InputProcessor">
        <property name="operandStackManager" ref="operandStackManager" />
        <property name="historyStackManager" ref="historyStackManager" />
    </bean>

    <bean id="operandStackManager" class="com.airwallex.rpncalculator.service.OperandStackManager" />
    <bean id="historyStackManager" class="com.airwallex.rpncalculator.service.HistoryStackManager" />

</beans>